public class Main {
    public static void main(String[] args) {
        int[] one = {1, 2, 3, 4};
        byte[] two = {1, 2, 3, 4};
        System.out.println(taskThree(one));
        System.out.println(taskThree(two));
    }

    static int taskThree(int... x){
        return x.length * 4;
    }

    static int taskThree(byte... x){
        return x.length;
    }
}
