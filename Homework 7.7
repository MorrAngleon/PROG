import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class Main {
    public static void main(String[] args) {
        Integer[] numbers = {2, 15, 42, 37, 8, 5, 20, 39, 13, 114};
        List<Integer> list = Arrays.asList(numbers);

        System.out.printf("Наименьшее значение элемента массива: %d", Collections.min(list));
        System.out.println();
        System.out.printf("Наибольшее значение элемента массива: %d", Collections.max(list));
    }
}
