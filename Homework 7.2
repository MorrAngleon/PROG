public class Main {
    public static void main(String[] args) {

        for (int x = 10; x <= 20; x++){
            System.out.print(x + " ");
        }

        System.out.println();

        int x = 10;
        while (x<=20){
            System.out.print(x + " ");
            x++;
        }

        System.out.println();

        int y = 10;
        do {
            System.out.print(y + " ");
            y++;
        } while (y<=20);
    }
}
