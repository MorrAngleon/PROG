public class Main {
    public static void main(String[] args) {
        int[] numbers = {2, 15, 42, 37, 8, 5, 20, 39, 13, 114};
        int sum = 0;
        for (int x = 0; x <numbers.length; x++) {
            if (x == 0 || x == numbers.length - 1)
                continue;
            sum += numbers[x];
        }
        System.out.println(sum);
    }
}
